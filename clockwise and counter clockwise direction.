import cv2
path = r"C:/Users/91934/Pictures/Screenshots/Screenshot 2023-10-17 101935.png"
src = cv2.imread(path)
window_name = 'Image'
image = cv2.rotate(src, cv2.ROTATE_90_COUNTERCLOCKWISE)
cv2.imshow(window_name, image)
cv2.waitKey(0)


